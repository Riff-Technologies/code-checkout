{
  "name": "@riff-tech/code-checkout",
  "version": "2.0.0",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "commonjs",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test",
    "example": "ts-node examples/basic-usage.ts"
  },
  "keywords": [
    "code-checkout",
    "license",
    "payment",
    "software",
    "subscription"
  ],
  "license": "MIT",
  "description": "code-checkout platform package to add licenses and payment support to your software.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Riff-Technologies/code-checkout.git",
    "directory": "examples"
  },
  "bugs": {
    "url": "https://github.com/Riff-Technologies/code-checkout/issues"
  },
  "homepage": "https://riff-tech.com/codecheckout",
  "author": "Riff Tech, LLC",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "uuid": "^9.0.1"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
